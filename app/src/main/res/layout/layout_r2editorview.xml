<?xml version="1.0" encoding="utf-8"?>
<work.pangbai.r2toolm.view.MVerticalScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:fillViewport="true"
    android:layout_width="match_parent"
    android:layout_marginBottom="45dp"
    android:id="@+id/scroll_v"
    android:layout_height="match_parent">
    <work.pangbai.r2toolm.view.MHorizontalScrollView
        android:overScrollMode="never"
        android:layout_width="match_parent"
        android:id="@+id/scroll_h"
        android:layout_height="match_parent">
        <work.pangbai.r2toolm.editor.R2EditorView
            android:id="@+id/editor"
            android:paddingLeft="5dp"
            android:selectAllOnFocus="false"
            android:textIsSelectable="true"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="!="
            />
    </work.pangbai.r2toolm.view.MHorizontalScrollView>
</work.pangbai.r2toolm.view.MVerticalScrollView>
